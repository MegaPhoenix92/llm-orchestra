{
  "name": "llm-orchestra",
  "version": "0.1.0",
  "description": "Unified Observability & Orchestration SDK for Multi-Model AI Applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:debug": "vitest --inspect-brk --single-thread",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "dashboard": "cd packages/dashboard && npm run dev"
  },
  "keywords": [
    "llm",
    "ai",
    "orchestration",
    "observability",
    "openai",
    "anthropic",
    "claude",
    "gpt",
    "gemini",
    "tracing",
    "multi-model"
  ],
  "author": "TROZLAN <info@trozlan.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MegaPhoenix92/llm-orchestra.git"
  },
  "bugs": {
    "url": "https://github.com/MegaPhoenix92/llm-orchestra/issues"
  },
  "homepage": "https://github.com/MegaPhoenix92/llm-orchestra#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "eslint": "^8.0.0",
    "tsx": "^4.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "openai": "^4.0.0",
    "@google/generative-ai": "^0.21.0"
  }
}
